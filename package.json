  "main": "index.js",
  "scripts": {
    "build": "pnpm run clean && pnpm run build:cjs && pnpm run build:esm && pnpm run build:deno",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:deno": "node ./deno/build.mjs && cp ./README.md ./deno/lib",
    "build:esm": "rollup --config rollup.config.js",
    "build:types": "tsc -p tsconfig.types.json",
    "clean": "rm -rf lib/* deno/lib/*",
    "dev": "ts-node-dev --transpile-only --respawn playground.ts",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "lint:check": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "rollup": "rollup --config rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:watch:verbose": "jest --watchAll --verbose"
  },
  "browserslist": [
    "defaults and supports es6-module",
    "maintained node versions"
  ],
  "dependencies": {
    "clone-deep": "^4.0.1",
    "dayjs": "^1.11.7",
    "is-plain-object": "^5.0.0",
    "merge-deep": "^3.0.3",
    "micro-memoize": "^4.0.14",
    "nanoid": "3.3.4",
    "safe-json-stringify": "^1.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/clone-deep": "^4.0.1",
    "@types/jest": "^29.2.4",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^18.11.16",
    "@types/safe-json-stringify": "^1.1.2",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.30.0",
    "eslint-plugin-compat": "^4.0.2",
    "expect-type": "^0.15.0",
    "fp-ts": "^2.13.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "ts-jest": "^29.0.3",
    "ts-toolbelt": "^9.6.0",
    "type-fest": "^3.4.0",
    "typescript": "^4.9.4"
  }
}
